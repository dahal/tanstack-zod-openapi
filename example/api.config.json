{
  "tagOrder": [
    "Health",
    "Authentication", 
    "Users",
    "Posts",
    "Comments"
  ],
  "crudOrder": {
    "POST": 1,
    "GET_LIST": 2,
    "GET_SINGLE": 3,
    "PUT": 4,
    "PATCH": 5,
    "DELETE": 6
  },
  "httpMethodOrder": {
    "POST": 1,
    "GET": 2,
    "PUT": 3,
    "PATCH": 4,
    "DELETE": 5
  },
  "options": {
    "includeTimestamp": true,
    "includeWarning": true,
    "groupByTag": true,
    "subgroupByCrud": true,
    "includeMethodComments": true,
    "includePathComments": true,
    "detectDuplicates": true,
    "warnMissingTags": true,
    "uncategorizedLast": true,
    "uncategorizedTagName": "Uncategorized"
  },
  "pathPatterns": {
    "singleResource": ["$id", "{id}", ":id"],
    "nestedResource": ["$", "{", ":"],
    "action": [".", "/"]
  },
  "excludePatterns": [
    "**/test/**",
    "**/mock/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}